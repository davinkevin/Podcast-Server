{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "rebaseWhen": "never",
  "assignees": [
    "@davinkevin"
  ],
  ignorePaths: [
    "frontend-angular/**",
    "frontend-angularjs/**"
  ],
  "packageRules": [{
    matchPackageNames: ["software.amazon.awssdk:bom"],
    "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
    automerge: true,
  }]
}

